{
  "presets": [
    "react",
    [
      "es2015",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [["module-resolver", { "root": ["./src"] } ]],
  "env": {
    "production": {
      "plugins": [
        "external-helpers"
      ]
    },
    "development": {
       "plugins": ["react-hot-loader/babel"]
    }
  }
}